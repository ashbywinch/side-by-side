{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":0,"text":"Под большим шатром\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":19,"text":"Голубых небес \n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":34,"text":"Вижу  даль степей\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":52,"text":"Зеленеется.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":64,"text":"И на гранях их,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":80,"text":"Выше темных туч,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":97,"text":"Цепи гор стоят\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":112,"text":"Великанами.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":124,"text":"По степям в моря\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":141,"text":"Реки катятся,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":155,"text":"И лежат пути\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":168,"text":"Во все стороны.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":184,"text":"Посмотрю на юг \n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":200,"text":"Нивы зрелые,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":213,"text":"Что камыш густой,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":231,"text":"Тихо движутся;\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":246,"text":"Мурава лугов\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":259,"text":"Ковром стелется,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":276,"text":"Виноград в садах\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":293,"text":"Наливается.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":305,"text":"Гляну к северу \n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":321,"text":"Там, в глуши пустынь,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":343,"text":"Снег, что белый пух,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":364,"text":"Быстро кружится;\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":381,"text":"Подымает грудь\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":396,"text":"Море синее,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":408,"text":"И горами лед\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":421,"text":"Ходит по морю;\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":436,"text":"И пожар небес\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":450,"text":"Ярким заревом\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":464,"text":"Освещает мглу\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":478,"text":"Непроглядную...\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":494,"text":"Это ты, моя\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":506,"text":"Русь державная,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":522,"text":"Моя родина\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":533,"text":"Православная!\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":547,"text":"Широко ты, Русь,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":564,"text":"По лицу земли\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":578,"text":"В красе царственной\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":598,"text":"Развернулася!\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":612,"text":"У тебя ли нет\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":626,"text":"Поля чистого,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":640,"text":"Где б разгул нашла\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":659,"text":"Воля смелая?\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":672,"text":"У тебя ли нет\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":686,"text":"Про запас казны,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":703,"text":"Для друзей стола,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":721,"text":"Меча недругу?\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":735,"text":"У тебя ли нет\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":749,"text":"Богатырских сил,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":766,"text":"Старины святой,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":782,"text":"Громких подвигов?\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":800,"text":"Перед кем","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":820,"text":"себя\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":815,"text":"Ты унизила?\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":827,"text":"Кому в черный день\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":846,"text":"Низко кланялась?\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":863,"text":"На полях своих,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":879,"text":"Под курганами,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":894,"text":"Положила ты\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":906,"text":"Татар полчища.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":921,"text":"Ты на жизнь и смерть\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":942,"text":"Вела спор с Литвой\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":961,"text":"И дала урок\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":973,"text":"Ляху гордому.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":987,"text":"И давно ль было,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1004,"text":"Когда с Запада\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1019,"text":"Облегла тебя\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1032,"text":"Туча темная?\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1045,"text":"Под грозой ее\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1059,"text":"Леса падали,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1072,"text":"Мать сыра-земля\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1088,"text":"Колебалася,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1100,"text":"И зловещий дым\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1115,"text":"От горевших сел\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1131,"text":"Высоко вставал\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1146,"text":"Черным облаком!\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1162,"text":"Но лишь кликнул царь\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1183,"text":"Свой народ на брань \n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1204,"text":"Вдруг со всех концов\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1225,"text":"Поднялася Русь.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1241,"text":"Собрала детей,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1256,"text":"Стариков и жен,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1272,"text":"Приняла гостей\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1287,"text":"На кровавый пир.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1304,"text":"И в глухих степях,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1323,"text":"Под сугробами,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1338,"text":"Улеглися спать\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1353,"text":"Гости навеки.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1367,"text":"Хоронили их\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1379,"text":"Вьюги снежные,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1394,"text":"Бури севера\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1406,"text":"О них плакали!..\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1423,"text":"И теперь среди\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1438,"text":"Городов твоих\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1452,"text":"Муравьем кишит\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1467,"text":"Православный люд.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1485,"text":"По седым морям\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1500,"text":"Из далеких стран\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1517,"text":"На поклон к тебе\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1534,"text":"Корабли идут.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1548,"text":"И поля цветут,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1563,"text":"И леса шумят,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1577,"text":"И лежат в земле\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1593,"text":"Груды золота.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1607,"text":"И во всех концах\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1624,"text":"Света белого\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1637,"text":"Про тебя идет\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1651,"text":"Слава громкая.\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1666,"text":"Уж и есть за что,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1684,"text":"Русь могучая,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1698,"text":"Полюбить тебя,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1713,"text":"Назвать матерью,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1730,"text":"Стать за честь твою\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1750,"text":"Против недруга,\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1766,"text":"За тебя в нужде\n","translation":""}
{"filename":"books/Иван Никитин/Русь_4451.txt","index_in_file":1782,"text":"Сложить голову!\n","translation":""}
