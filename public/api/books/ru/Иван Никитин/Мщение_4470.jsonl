{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":0,"text":"Поднялась, шумит\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":17,"text":"Непогодушка,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":30,"text":"Низко","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":42,"text":"бор сырой\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":46,"text":"Наклоняется.\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":59,"text":"Ходят, плавают\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":74,"text":"Тучи по небу,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":88,"text":"Ночь осенняя\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":101,"text":"Черней ворона.\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":116,"text":"В зипуне мужик\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":131,"text":"К дому барскому\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":147,"text":"Через сад густой\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":164,"text":"Тихо крадется.\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":179,"text":"Он идет, глядит\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":195,"text":"Во все стороны,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":211,"text":"Про себя один\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":225,"text":"Молча думает:\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":239,"text":"Вот теперь с тобой,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":259,"text":"Барин-батюшка,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":274,"text":"Мужик-лапотник\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":289,"text":"Посчитается;\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":302,"text":"Хорошо ты мне\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":316,"text":"Вчера вечером\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":330,"text":"Вплоть до плеч спустил\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":353,"text":"Кожу бедную.\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":366,"text":"Виноват я был,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":381,"text":"Сам ты ведаешь:\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":397,"text":"Тебе дочь моя\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":411,"text":"Приглянулася.\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":425,"text":"Да отец ее \n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":437,"text":"Несговорчивый,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":452,"text":"Не велит он ей\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":467,"text":"Слушать барина...\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":485,"text":"Знаю, ты у нас\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":500,"text":"Сам большой-старшой,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":521,"text":"И судить-","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":539,"text":"рядить\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":537,"text":"Тебя некому.\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":550,"text":"Так суди ж, господь,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":571,"text":"Меня, грешника:\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":587,"text":"Не видать тебе\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":602,"text":"Мое детище!\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":614,"text":"Подошел мужик\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":628,"text":"К дому барскому,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":645,"text":"Тихо выломил\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":658,"text":"Раму старую,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":671,"text":"Поднялся, вскочил\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":689,"text":"В спальню темную, \n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":708,"text":"Не вставать теперь\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":727,"text":"Утром барину...\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":743,"text":"На дворе шумит\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":758,"text":"Непогодушка,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":771,"text":"Низко","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":783,"text":"бор сырой\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":787,"text":"Наклоняется;\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":800,"text":"Через сад домой\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":816,"text":"Мужик крадется,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":832,"text":"У него лицо\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":844,"text":"Словно белый снег.\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":863,"text":"Он дрожит как лист,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":883,"text":"Озирается,\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":894,"text":"А господский дом\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":911,"text":"Загорается.\n","translation":""}
{"filename":"books/Иван Никитин/Мщение_4470.txt","index_in_file":923,"text":"\n","translation":""}
